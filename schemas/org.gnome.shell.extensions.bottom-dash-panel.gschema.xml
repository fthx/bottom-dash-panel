<?xml version="1.0" encoding="UTF-8"?>

<schemalist>

    <schema path="/org/gnome/shell/extensions/bottom-dash-panel/" id="org.gnome.shell.extensions.bottom-dash-panel">

        <key type="d" name="dash-height">
            <default>4.4</default>
            <range min="1" max="15"/>
            <summary>Dash max height (%)</summary>
        </key>

        <key type="i" name="dash-background-opacity">
            <default>100</default>
            <range min="0" max="100"/>
            <summary>Dash background opacity (%)</summary>
        </key>

    </schema>

</schemalist>
